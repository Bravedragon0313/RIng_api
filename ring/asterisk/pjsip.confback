[global]
user_agent=Android/1.10.14 (belle-sip/1.4.2)
type=global

;================================ TRANSPORTS ==
; Our primary transport definition for UDP communication behind NAT.
[transport-udp]
type = transport
protocol = udp
bind = 0.0.0.0
; NAT settings
;local_net = 10.0.0.0/8
;external_media_address = 203.0.113.1
;external_signaling_address = 203.0.113.1

[transport-tls]
type=transport
protocol=tls
bind = 0.0.0.0
method=tlsv1

